{
    "name": "toumoro/tm-migration",
    "type": "typo3-cms-extension",
    "description": "TYPO3 extension that brings together the tools used for a major TYPO3 migration.",
    "keywords": [
        "TYPO3",
        "Toumoro",
        "Migration"
    ],
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/toumoro/tm_migration",
    "authors": [
        {
            "name": "Mehdi Guermazi",
            "email": "mehdi.guermazi@toumoro.com"
        },
        {
            "name": "Haythem Daoud",
            "email": "haythem.daoud@toumoro.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "typo3/cms-core": "^12.4.0 || ^13.4.0",
        "a9f/typo3-fractor": "^v0.5.1",
        "ssch/typo3-rector": "^v3.5.0",
        "wapplersystems/core-upgrader": "dev-release/v13"
    },
    "autoload": {
        "psr-4": {
            "Toumoro\\TmMigration\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toumoro\\TmMigration\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/tm-migration": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "tm_migration"
        }
    }    
}
